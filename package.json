{
    "name": "@noia-network/node",
    "version": "0.3.15",
    "description": "NOIA Node core.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npm run build-ts",
        "pretest": "tsc -p . --noEmit",
        "test": "echo 'Warning: jest tests are skipped.'",
        "posttest": "npm run prettier",
        "watch-jest": "jest --watch",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "prettier": "prettier \"src/**/*\" -l"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/noia-network/noia-node.git"
    },
    "keywords": [],
    "author": {
        "name": "NOIA Network Limited"
    },
    "license": "LGPL-2.1",
    "homepage": "https://github.com/noia-network/noia-node",
    "devDependencies": {
        "jest": "^22.4.3",
        "prettier": "^1.13.5",
        "@types/jest": "^22.2.3",
        "typescript": "^2.8.3",
        "ts-jest": "^22.4.4"
    },
    "dependencies": {
        "@noia-network/governance": "0.0.4",
        "@noia-network/node-contents-client": "^0.2.1",
        "@noia-network/protocol": "^0.3.2",
        "@noia-network/webrtc-direct-server": "^0.2.7",
        "@types/express": "^4.16.0",
        "@types/jsonfile": "^4.0.1",
        "@types/mime-types": "^2.1.0",
        "@types/node": "^9.6.12",
        "@types/node-fetch": "^1.6.8",
        "@types/request": "^2.47.1",
        "@types/rimraf": "^2.0.2",
        "@types/ws": "^4.0.2",
        "body-parser": "^1.18.2",
        "crypto": "^1.0.1",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "external-ip": "^2.0.3",
        "jsonfile": "^4.0.0",
        "logdna-winston": "^1.0.0",
        "mime-types": "^2.1.18",
        "node-fetch": "^2.0.0",
        "randombytes": "^2.0.6",
        "request": "^2.85.0",
        "rimraf": "^2.6.2",
        "swagger-ui-express": "^3.0.8",
        "tslib": "^1.9.2",
        "winston": "^2.4.1",
        "ws": "^5.0.0"
    },
    "files": [
        "dist",
        "**/*.md",
        "swagger.json",
        "@types",
        "!/examples"
    ],
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx)?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
